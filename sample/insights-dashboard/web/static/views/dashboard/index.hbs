<div class="wrapper">

    {{> default/menu }}

    <div class="content-wrapper">
        <section class="content-header">
        <div class="row">
        <div class="col-xs-12">
            <div class="opt_fr">
                <ol class="breadcrumb dashboard-date-range mar_0">
                    <li>
                        <select multiple="true" class="select-box-large institution"
                            data-placeholder="All Institutions"></select>
                    </li>
                    <li>
                        <select class="store-select-box">
                            {{#each user.stores}}
                                <option value="{{id}}">{{name}}</option>
                            {{/each}}
                        </select>
                    </li>
                    <li>
                        <i class="fa fa-calendar glyphicon glyphicon-calendar"></i>
                        <input type="text" class="form-control drpicker-input" name="daterange" value="" />
                    </li>
                </ol>
            </div>
        </div>
        </div>
        </section>

        <section class="content">
            <div class="row">
                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="info-box">
                        <span class="info-box-icon">
                            <i class="fa fa-laptop"></i>
                        </span>
                        <div class="info-box-content">
                            <span class="info-box-text">Total Events</span>
                            <span class="info-box-number eventcount">0</span>
                        </div>
                    </div>
                </div>

                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="info-box">
                        <span class="info-box-icon">
                            <i class="fa fa-tasks"></i>
                        </span>
                        <div class="info-box-content">
                            <span class="info-box-text">Sessions</span>
                            <span class="info-box-number sessions">0</span>
                        </div>
                    </div>
                </div>
                <div class="clearfix visible-sm-block"></div>

                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="info-box">
                        <span class="info-box-icon">
                            <i class="fa fa-user"></i>
                        </span>
                        <div class="info-box-content">
                            <span class="info-box-text">New Users</span>
                            <span class="info-box-number newusers">0</span>
                        </div>
                    </div>
                </div>

                <div class="col-md-3 col-sm-6 col-xs-12">
                    <div class="info-box">
                        <span class="info-box-icon">
                            <i class="fa fa-users"></i>
                        </span>
                        <div class="info-box-content">
                            <span class="info-box-text">Active Users</span>
                            <span class="info-box-number uniqueusers">0</span>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-6">
                    <div class="box">
                        <div class="box-header">
                            <h3 class="box-title">Daily Usage</h3>
                             <span class="box-title-help" name="" data-toggle="tooltip" data-placement="top" title="The count of sessions, active users and new users for each day, for the specified duration"><i class="fa fa-question-circle" aria-hidden="true"></i></span>
                        </div>
                        <div class="daily-app-usage">
                            <div class="loading">
                                <div class="spinner">
                                    <div class="rect1"></div>
                                    <div class="rect2"></div>
                                    <div class="rect3"></div>
                                    <div class="rect4"></div>
                                    <div class="rect5"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-md-6">
                    <div class="box">
                        <div class="box-header">
                            <h3 class="box-title">Application Version</h3>
                            <span class="box-title-help" name="" data-toggle="tooltip" data-placement="top" title="The count of all activities which were performed on specific application versions"><i class="fa fa-question-circle" aria-hidden="true"></i></span>
                        </div>
                        <div class="app_version">
                            <div class="loading">
                                <div class="spinner">
                                    <div class="rect1"></div>
                                    <div class="rect2"></div>
                                    <div class="rect3"></div>
                                    <div class="rect4"></div>
                                    <div class="rect5"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-6">
                    <div class="box">
                        <div class="box-header">
                            <h3 class="box-title">Book Open - Apps</h3>
                            <span class="box-title-help" name="" data-toggle="tooltip" data-placement="top" title="The count of books opened using the reader app, on any day, for the specified duration"><i class="fa fa-question-circle" aria-hidden="true"></i></span>
                        </div>
                        <div class="book-open">
                            <div class="loading">
                                <div class="spinner">
                                    <div class="rect1"></div>
                                    <div class="rect2"></div>
                                    <div class="rect3"></div>
                                    <div class="rect4"></div>
                                    <div class="rect5"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-md-6">
                    <div class="box">
                        <div class="box-header">
                            <h3 class="box-title">Device Platforms</h3>
                            <span class="box-title-help" name="" data-toggle="tooltip" data-placement="top" title="Different types of devices used to access the reader app is displayed in percentage"><i class="fa fa-question-circle" aria-hidden="true"></i></span>
                        </div>
                        <div class="device_platform">
                            <div class="loading">
                                <div class="spinner">
                                    <div class="rect1"></div>
                                    <div class="rect2"></div>
                                    <div class="rect3"></div>
                                    <div class="rect4"></div>
                                    <div class="rect5"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>
